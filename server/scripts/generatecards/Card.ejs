const Card = require('../../Card.js');

class <%= name %> extends Card {
    setupCardAbilities(ability) {
        <%_ for(var ability of abilities) { _%>
        <%- include('Ability', {ability: ability}); _%>
        <%_ } _%>
    }
}

<%= name %>.id = '<%= card.id %>';

module.exports = <%= name %>;
